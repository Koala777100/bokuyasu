<h2>Create Member</h2>
<%= form_with model: @member, local:true do |f| %>
  <p>Name</p>
  <%= f.text_field :name, class: 'form-control' %>

  <p>Image</p>
  <%= f.attachment_field :profile_image %>

  <p>Introduction</p>
  <%= f.text_area :introduction, class: 'form-control' %></br>
  <%= f.submit 'Create Members', class: "btn btn-secondary" %>
<% end %>
