<h2>Create News</h2>
<%= form_with model: @blog, local:true do |f| %>
  <p>Title</p>
  <%= f.text_field :title, class: 'form-control' %>

  <p>Image</p>
  <%= f.attachment_field :blog_image %>

  <p>Body</p>
  <%= f.text_area :body, class: 'form-control' %></br>

  <%= f.submit 'Create News', class: "btn btn-secondary" %>
<% end %>
